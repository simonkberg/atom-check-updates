{
  "name": "atom-check-updates",
  "version": "0.1.2",
  "description": "CLI for easily updating Atom on RPM or Debian-based systems.",
  "main": "lib",
  "preferGlobal": true,
  "engines": {
    "node": ">=4.2.0",
    "npm": ">=2.0.0"
  },
  "os": [
    "linux"
  ],
  "cpu": [
    "x64"
  ],
  "bin": {
    "acu": "lib",
    "atom-check-updates": "lib"
  },
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "nyr lint -- --fix",
    "test": "nyr lint",
    "build": "babel src -d lib",
    "build:watch": "nyr build -- -w",
    "prepublish": "nyr build"
  },
  "repository": "simonkberg/atom-check-updates",
  "keywords": [
    "atom",
    "cli",
    "debian",
    "update",
    "ubuntu",
    "redhat",
    "rpm"
  ],
  "author": "Simon Kjellberg <simon.kjellberg@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@simonkberg/eslint-config": "^6.0.0",
    "babel-cli": "^6.7.5",
    "babel-preset-env": "^1.4.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^3.19.0",
    "nyr": "^1.1.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "inquirer": "^3.0.6",
    "progress": "^2.0.0",
    "request": "^2.71.0"
  },
  "files": [
    "index.js",
    "lib/acu.js",
    "LICENSE",
    "readme.md"
  ]
}
