{
  "name": "atom-check-updates",
  "version": "0.1.2",
  "description": "CLI for easily updating Atom on RPM or Debian-based systems.",
  "main": "index.js",
  "preferGlobal": true,
  "engines": {
    "node": ">=4.2.0",
    "npm": ">=2.0.0"
  },
  "os": ["linux"],
  "cpu": ["x64"],
  "bin": {
    "acu": "index.js",
    "atom-check-updates": "index.js"
  },
  "scripts": {
    "lint": "eslint src",
    "build": "babel src -d lib",
    "build:watch": "npm run build -- -w",
    "prepublish": "npm run build"
  },
  "repository": "simonkberg/atom-check-updates",
  "keywords": [
    "atom",
    "cli",
    "debian",
    "update",
    "ubuntu",
    "redhat",
    "rpm"
  ],
  "author": "Simon Kjellberg <simon.kjellberg@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.8.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "inquirer": "^1.0.1",
    "progress": "^1.1.8",
    "request": "^2.71.0"
  },
  "files": [
    "index.js",
    "lib/acu.js",
    "LICENSE",
    "readme.md"
  ]
}
